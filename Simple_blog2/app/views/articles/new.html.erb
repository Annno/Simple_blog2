<h1>Create a new article</h1>
<%= form_for(@article) do |f| %>
	<ul>
	<% @article.errors.full_messages.each do |error| %>
	<li><%= error %></li>
	<% end %>
	</ul>
	<p>
		<%= f.label :Title %><br>
		<%= f.text_field :title %>
	</p>
	<p>
		<%= f.label :tag_list %><br>
		<%= f.text_field :tag_list%>
	</p>
	<p>
		<%= f.label :Body %><br>
		<%= f.text_field :body %>
	</p>
	<p>
		<%= f.submit %>
	</p>
<% end %>





